      <body>
          <div id="main-menu">
              <div id="comdetails">
                <h1>Zeus Agent</h1>
              </div>
              <div id="navbar">
                  <h3>Successfully Created Sticky Note!</h3>
                  <div style="text-align: center;">
                  <input id="url" readonly style="margin-bottom:50px" value="localhost:8080/<%= data %>"><button onclick="myFunction()" style="font-weight: bolder;">Copy URL</button> <br>
                  <a href="/home">
                      <input type="button" name="goBack" id="goBack" value="Go Back" style="font-weight:bolder;color:black;background-color:#e4e4e4; width: fit-content;">
                  </a>
                  <a href="/logout">
                      <input type="button" name="logout" id="logout" value="Logout" style="font-weight:bolder;color:black;background-color:#e4e4e4; width: fit-content;">
                  </a>
